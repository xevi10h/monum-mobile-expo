{
	"cli": {
		"version": ">= 12.3.0",
		"appVersionSource": "remote"
	},
	"build": {
		"development": {
			"developmentClient": true,
			"distribution": "internal"
		},
		"development-simulator": {
			"developmentClient": true,
			"distribution": "internal",
			"ios": {
				"simulator": true
			}
		},
		"preview": {
			"distribution": "internal"
		},
		"production": {
			"autoIncrement": true
		}
	},
	"submit": {
		"production": {
			"android": {
				"serviceAccountKeyPath": "../path/to/api-xxx-yyy-zzz.json",
				"track": "internal"
			},
			"ios": {
				"appleId": "john@turtle.com",
				"ascAppId": "1234567890",
				"appleTeamId": "AB12XYZ34S"
			}
		}
	}
}
